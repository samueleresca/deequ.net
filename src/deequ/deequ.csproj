<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>netstandard2.1</TargetFramework>
        <RootNamespace>xdeequ</RootNamespace>
      <GenerateDocumentationFile>true</GenerateDocumentationFile>
      <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
      <Copyright />
      <Version>0.0.1-alpha</Version>
      <PackageLicenseUrl></PackageLicenseUrl>
      <RepositoryUrl>https://github.com/samueleresca/deequ.NET</RepositoryUrl>
      <Authors>Samuele Resca</Authors>
      <Company />
      <PackageProjectUrl>https://github.com/samueleresca/deequ.NET</PackageProjectUrl>
      <AssemblyVersion>1.1.0.0</AssemblyVersion>
      <FileVersion>1.1.0.0</FileVersion>
      <PackageTags>deequ.NET is a port of the awslabs/deequ library built on top of Apache Spark for defining "unit tests for data", which measure data quality in large datasets.</PackageTags>
      <PackageLicenseExpression>MIT</PackageLicenseExpression>
    </PropertyGroup>
    <ItemGroup>
        <PackageReference Include="Microsoft.Spark" Version="0.11.0" />
        <PackageReference Include="Microsoft.Spark.Experimental" Version="0.11.0" />
        <PackageReference Include="Microsoft.Spark.Extensions.Delta" Version="0.11.0" />
        <PackageReference Include="System.Text.Json" Version="4.7.2" />
    </ItemGroup>
</Project>